	function printOffers(json){
		
		$("#result-list-jobs").children().remove();		
		$.each($.parseJSON(json), function(idx, obj) {
			
			var url = "http://empleo.nortempo.com/buscar-ofertas?p_p_id=Buscadordeofertas_WAR_Candidatosportlet&p_p_lifecycle=0&p_p_state=normal&p_p_mode=view&p_p_col_id=column-1&p_p_col_pos=1&p_p_col_count=2&_Buscadordeofertas_WAR_Candidatosportlet_action=viewOffer" + "&idOffer=" + obj.id + "&search=" + $('[name="buscarTrabajo"]').val() + "&paginator=" + $('#PaginatorNumber').text()+"&filtroCategoria="+ filterCategoriaList.join("-separatorsnt-")+"&filtroSubcategoria="+filterSubcategoriaList.join("-separatorsnt-")+"&filtroProvincias="+filterProvinciaList.join("-separatorsnt-")+"&filtroJornadas="+filterJornadaList.join("-separatorsnt-");	
			
			var str = '<li data-categoria="'+ obj.categoriaId.name +'" data-subcategoria="'+ obj.subcategoriaId.name+'" data-provincia="" data-jornada="'+ obj.jornadaLaboralId.name +'">';
			str += '<div class="details-job"><header><h6><a href="' + url +'">' + obj.titulo + '</a></h6></header>';
			str += '<div class="details-job-description">';
			if (obj.oficina == null){
				str += 'Nortempo. ';	
			}else{
				str += obj.oficina.replace('Oficina','Nortempo')+". ";				
			}
			if (obj.paisId.codigo != '108'){
				str += obj.paisId.name;
			}
			if(obj.localidadesList.length > 0 ){
				str += ' ';
				$.each( obj.localidadesList , function( index, value ){
					if( index < obj.localidadesList.length && index > 0 ){
						str += ', ';
					}
					str += value.name;
				});
				str += ' ';
			}else{
				if( obj.provinciasList.length > 0 ){
					$.each( obj.provinciasList , function( index, value ){
						if( index < obj.provinciasList.length && index > 0 ){
							str += ', ';
						}
						str += value.name;
					});
				}
			}
			if( obj.provinciasList.length > 0 ){
				str += ' (';
				$.each( obj.provinciasList , function( index, value ){
					if( index < obj.provinciasList.length && index > 0 ){
						str += ', ';
					}
					str += value.name;
				});
				str += ') ';
			}
			str += '</div>';		
			str += '<div class="details-job-description">'+ $.format.date( new Date( obj.fechaPublicacion ) , "dd/MM/yyyy") +'</div>';
			str += '<div class="details-job-footer">' + obj.descripcionEncabezado +'</div>';
			str += '</div></li>';
			
			$("#result-list-jobs").append( $(str) );
			controlPrevNext();

		});
	}
	
